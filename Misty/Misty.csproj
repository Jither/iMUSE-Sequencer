    </Content>
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Jither.Imuse\Jither.Imuse.csproj" />
    <ProjectReference Include="..\Jither.Midi\Jither.Midi.csproj" />
  </ItemGroup>

</Project>
