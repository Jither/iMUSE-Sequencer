    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>disable</Nullable>
    <Description>MIDI and iMUSE Spelunking Tool - Yowza!</Description>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="default.mapping" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="default.mapping">
